@model OnlineBankingForManagers.WebUI.Models.ClientsListViewModel

@{
  ViewBag.Title = "Clients";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="categories">
    @{ Html.RenderAction("Menu", "Nav"); }
</div>
<table>
    @foreach (var c in Model.Clients)
    {
        <tr>
            @Html.ActionLink(c.ContractNumber, "Edit", new { c.ClientId });

            @{
        Html.RenderPartial("ClientSummary", c);
            }
        </tr>

    }
</table>
<div class="pager">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List",
        new {page = x, address = Model.CurrentAddress}))
</div>

 